{
  "name": "guide-to-the-us",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy:web": "./deploy-web.sh",
    "publish": "expo publish",
    "update": "eas update --auto",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "commit": "./quick-commit.sh",
    "push": "git push origin main",
    "deploy": "./deploy-complete.sh",
    "deploy:quick": "npm run commit 'Quick update' && npm run update",
    "version:patch": "npm version patch && git push origin main --tags",
    "version:minor": "npm version minor && git push origin main --tags",
    "version:major": "npm version major && git push origin main --tags"
  },
  "dependencies": {
    "@expo/cli": "^0.24.13",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "eas-cli": "^16.6.2",
    "expo": "~53.0.9",
    "expo-dev-client": "~5.1.8",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.0",
    "react-native-vector-icons": "^10.2.0",
    "react-dom": "19.0.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.4",
    "expo-updates": "~0.28.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
